<?xml version="1.0"?>
<!DOCTYPE liferay-portlet-app PUBLIC "-//Liferay//DTD Portlet Application 6.0.0//EN" "http://www.liferay.com/dtd/liferay-portlet-app_6_0_0.dtd">

<liferay-portlet-app>
	<portlet>
		<portlet-name>configurationtemplate</portlet-name>
		<icon>/icon.png</icon>
		<instanceable>true</instanceable>
		<header-portlet-css>/css/oi.configurationtemplate.css</header-portlet-css>
        <header-portlet-css>/css/plugins/ui.jqgrid.css</header-portlet-css>
		<header-portlet-javascript>/js/plugins/jquery.metadata.js</header-portlet-javascript>
		<header-portlet-javascript>/js/plugins/i18n/grid.locale-en.js</header-portlet-javascript>
		<header-portlet-javascript>/js/plugins/jquery.jqGrid.min.js</header-portlet-javascript>
        <header-portlet-javascript>/js/configurationtemplate/init.js</header-portlet-javascript>
		<header-portlet-javascript>/js/configurationtemplate/application.js</header-portlet-javascript>
        <footer-portlet-javascript>/js/configurationtemplate/dialog/dialog.template.js</footer-portlet-javascript>
        <footer-portlet-javascript>/js/configurationtemplate/dialog/dialog.element.js</footer-portlet-javascript>
	</portlet>
	<role-mapper>
		<role-name>administrator</role-name>
		<role-link>Administrator</role-link>
	</role-mapper>
	<role-mapper>
		<role-name>guest</role-name>
		<role-link>Guest</role-link>
	</role-mapper>
	<role-mapper>
		<role-name>power-user</role-name>
		<role-link>Power User</role-link>
	</role-mapper>
	<role-mapper>
		<role-name>user</role-name>
		<role-link>User</role-link>
	</role-mapper>
</liferay-portlet-app>